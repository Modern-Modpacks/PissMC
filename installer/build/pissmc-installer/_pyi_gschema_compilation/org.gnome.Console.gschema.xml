<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="kgx">
  <enum id="org.gnome.Console.Theme">
    <value nick="auto" value="0"/>
    <value nick="night" value="1"/>
    <value nick="day" value="2"/>
    <value nick="hacker" value="3"/>
  </enum>

  <schema id="org.gnome.Console" path="/org/gnome/Console/">
    <key name="theme" enum="org.gnome.Console.Theme">
      <default>'night'</default>
    </key>
    <key name="font-scale" type="d">
      <range min="0.5" max="4.0"/>
      <default>1.0</default>
    </key>
    <key name="shell" type="as">
      <default>[]</default>
    </key>
    <key name="scrollback-lines" type="x">
      <default>10000</default>
    </key>
  </schema>
</schemalist>
